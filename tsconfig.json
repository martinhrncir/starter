{
    "compilerOptions": {
        "outDir": "./dist/",
        "module": "esnext",
        "target": "es6",
        "moduleResolution": "node",
        "baseUrl": "resources/assets/js",
        "allowSyntheticDefaultImports": true,
        "allowJs": true,
        "noImplicitAny": false,
        "sourceMap": true,
        "jsx": "react",
        "lib": [ "es2015", "dom" ],
    },
    "include": [
        "./src/**/*"
    ]
}
